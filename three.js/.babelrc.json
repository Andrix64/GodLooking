{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": ">0.3%, not dead",
        "loose": true,
        "bugfixes": true
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ]
  ]
}
